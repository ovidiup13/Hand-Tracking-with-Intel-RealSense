<Window x:Class="HandTracking.MenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HandTracking"
        mc:Ignorable="d"
        Title="Menu" Height="320" Width="303">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="68*"/>
            <ColumnDefinition Width="113*"/>
            <ColumnDefinition Width="114"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" Content="Something something &#xD;&#xA;dark side" HorizontalAlignment="Left" Height="43" Margin="0,22,0,0" VerticalAlignment="Top" Width="163" Grid.ColumnSpan="2" Grid.Column="1"/>
        <Button x:Name="button" Content="Start New Experiment" HorizontalAlignment="Left" VerticalAlignment="Top" Width="163" Margin="0,115,0,0" Click="button_Click" Grid.ColumnSpan="2" Grid.Column="1"/>
        <Button x:Name="button_Copy" Content="Load Experiment" HorizontalAlignment="Left" VerticalAlignment="Top" Width="163" Margin="0,150,0,0" Grid.ColumnSpan="2" Grid.Column="1"/>
        <Button x:Name="button_Copy1" Content="Quit" HorizontalAlignment="Left" VerticalAlignment="Top" Width="163" Margin="0,184,0,0" Grid.ColumnSpan="2" Grid.Column="1"/>

    </Grid>
</Window>
