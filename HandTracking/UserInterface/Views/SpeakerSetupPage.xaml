<UserControl x:Class="UserInterface.Views.SpeakerSetupPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:controls="clr-namespace:UserInterface.Controls"
             xmlns:content="clr-namespace:UserInterface.Views.Content"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             DataContext="{Binding Source={StaticResource ViewModelLocator}, Path=SpeakerViewModel}">
    <Grid Style="{StaticResource ContentRoot}" DataContext="">
        <content:SpeakerList/>
    </Grid>
</UserControl>
